  "keywords": [
    "string-similarity",
    "string-comparison",
    "dice-coefficient",
    "strings"
  ],
  "version": "1.0.0",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Henrik9999\\StringSimilarity\\": "src"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "require": {
    "php": "^7.4|^8.0",
    "ext-mbstring": "*"
  }
}
